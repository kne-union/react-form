{
  "name": "@kne/react-form",
  "version": "0.1.3",
  "description": "用于表单验证的react组件",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "umd": "dist/react-form.js",
  "umd:min": "dist/react-form.min.js",
  "scripts": {
    "build": "rollup -c",
    "start-demo": "cd example&&npm start",
    "start": "rollup -c -w",
    "prettier": "prettier --config .prettierrc --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kne-union/react-form.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "form",
    "validate"
  ],
  "author": "linzp",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kne-union/react-form/issues"
  },
  "homepage": "https://github.com/kne-union/react-form#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "peerDependencies": {
    "prop-types": "15.x",
    "react": "16.x",
    "react-dom": "16.x"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@kne/use-callback-state": "^0.1.0",
    "fbemitter": "^2.1.1",
    "lodash": "^4.17.15",
    "use-debounce": "^3.4.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "babel-preset-react-app": "^9.1.0",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "rollup": "^1.27.14",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-terser": "^5.1.3"
  }
}
